<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ar.pay.wechatshare">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:name=".app.APP"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.main.HomeActivity"
            android:theme="@style/Notitle"
            android:windowSoftInputMode="adjustPan|stateHidden">>
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".module.main.ArticlesDetail"/>
        <activity android:name=".module.other.InfoActivity"/>
        <activity android:name=".module.other.MineArticlesActivity"/>
    </application>

</manifest>